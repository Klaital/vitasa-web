<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">ID</Data></Cell>
        <Cell><Data ss:Type="String">Subject</Data></Cell>
        <Cell><Data ss:Type="String">Details</Data></Cell>
        <Cell><Data ss:Type="String">Submitter</Data></Cell>
        <Cell><Data as:Type="String">From Public?</Data></Cell>
        <Cell><Data as:Type="String">Status</Data></Cell>
      </Row>
    <% @suggestions.each do |suggestion| %>
      <Row>
        <Cell><Data ss:Type="Number"><%= suggestion.id %></Data></Cell>
        <Cell><Data ss:Type="String"><%= suggestion.subject %></Data></Cell>
        <Cell><Data ss:Type="String"><%= suggestion.details %></Data></Cell>
        <Cell><Data ss:Type="String"><%= suggestion.user_id.nil? ? '' : suggestion.user.name %></Data></Cell>
        <Cell><Data ss:Type="Boolean"><%= suggestion.from_public %></Data></Cell>
        <Cell><Data ss:Type="String"><%= suggestion.status %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>
